<Project DefaultTargets="Build" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <ProjectGuid>{#PROJECTGUID}</ProjectGuid>
    <Configuration Condition=" '$(Configuration)' == '' ">Debug</Configuration>
    <Platform Condition=" '$(Platform)' == '' ">x86</Platform>
    <OutputType>Library</OutputType>
    <RootNamespace>{#NAMESPACE}</RootNamespace>
    <AssemblyName>{#NAMESPACE}</AssemblyName>
    <BaseIntermediateOutputPath>{#SOLUTIONHOME}/_bin/Server_Client/</BaseIntermediateOutputPath>
    <TargetFrameworkVersion>v2.0</TargetFrameworkVersion>
    <AllowUnsafeBlocks>False</AllowUnsafeBlocks>
    <NoStdLib>False</NoStdLib>
    <WarningLevel>4</WarningLevel>
    <TreatWarningsAsErrors>false</TreatWarningsAsErrors>
    <DocumentationFile>{#SOLUTIONHOME}/_bin/Server_Client/{#NAMESPACE}.xml</DocumentationFile>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)' == 'Debug' ">
    <OutputPath>{#SOLUTIONHOME}/_bin/Server_Client/Debug/</OutputPath>
    <DebugSymbols>true</DebugSymbols>
    <DebugType>Full</DebugType>
    <CheckForOverflowUnderflow>True</CheckForOverflowUnderflow>
    <DefineConstants>DEBUG;TRACE;DEBUGMODE</DefineConstants>
    <IntermediateOutputPath>{#SOLUTIONHOME}/_bin/Server_Client/Debug/</IntermediateOutputPath>
    <Optimize>False</Optimize>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)' == 'Release' ">
    <OutputPath>{#SOLUTIONHOME}/_bin/Server_Client/Release/</OutputPath>
    <DebugSymbols>false</DebugSymbols>
    <DebugType>None</DebugType>
    <CheckForOverflowUnderflow>True</CheckForOverflowUnderflow>
    <DefineConstants>TRACE</DefineConstants>
    <Optimize>False</Optimize>
    <IntermediateOutputPath>{#SOLUTIONHOME}/_bin/Server_Client/Release/</IntermediateOutputPath>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Platform)' == 'x86' ">
    <RegisterForComInterop>False</RegisterForComInterop>
    <GenerateSerializationAssemblies>Auto</GenerateSerializationAssemblies>
    <BaseAddress>4194304</BaseAddress>
    <PlatformTarget>x86</PlatformTarget>
    <FileAlignment>4096</FileAlignment>
  </PropertyGroup>
  <Import Project="$(MSBuildBinPath)\Microsoft.CSharp.Targets" />
  <ItemGroup>
    <Reference Include="Ict.Common">
      <HintPath>{#SOLUTIONHOME}/../../Common/_bin/Debug/Ict.Common.dll</HintPath>
      <SpecificVersion>False</SpecificVersion>
    </Reference>
    <Reference Include="Ict.Common.Data">
      <HintPath>{#SOLUTIONHOME}/../../Common/_bin/Debug/Ict.Common.Data.dll</HintPath>
      <SpecificVersion>False</SpecificVersion>
    </Reference>
    <Reference Include="System" />
    <Reference Include="System.Data" />
    <Reference Include="System.Xml" />
  </ItemGroup>
  <ItemGroup>
    <Compile Include="{#NAMESPACE}.AssemblyInfo.cs" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="{#SOLUTIONHOME}/Ict.Petra.Shared.csproj">
      <Project>{5F8FBEF6-2E30-4E12-B3E4-80BC29932B7D}</Project>
      <Name>Ict.Petra.Shared</Name>
    </ProjectReference>
  </ItemGroup>
</Project>