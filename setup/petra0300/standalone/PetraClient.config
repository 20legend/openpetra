<configuration>
  <system.runtime.remoting>
    <application>
	  <channels>
		<channel ref="tcp" port="0">
		  <clientProviders>
			<formatter ref="binary" />
		  </clientProviders>
		  <serverProviders>
			<formatter ref="binary" typeFilterLevel="Full" />
		  </serverProviders>
		</channel>
	  </channels>

	  <client>
		<wellknown type="Ict.Petra.Shared.Interfaces.IClientManagerInterface, Ict.Petra.Shared.Interfaces" url="tcp://localhost:9000/Clientmanager" />
	  </client>

	</application>
  </system.runtime.remoting>

  <appSettings>
	<add key="OpenPetra.PathTemp" value="TOREPLACE" />
	<add key="Reporting.PathReportSettings" value="TOREPLACE" />
    <add key="UINavigation.File" value="UINavigation.yml" />
    <add key="Resource.Dir" value="..\resources30" />
	<add key="UseWindowsUserID" value="true" />				<!-- Default Value: "false" -->
	<add key="DelayedDataLoading" value="true" />				<!-- Default Value: "false" -->
	
	<add key="ServerPollIntervalInSeconds" value="5" />			<!-- Default Value: "5" -->
	<add key="ServerObjectKeepAliveIntervalInSeconds" value="10" />		<!-- Default Value: "10" -->
	
	<add key="RunAsStandalone" value="true" />				<!-- Default Value: "false" -->
	<add key="RunAsRemote" value="false" />					<!-- Default Value: "false" -->

    <add key="PetraServerAdmin.Configfile" value="PetraServerAdminConsole.exe.config" />
    <add key="PetraServer.Configfile" value="PetraServerConsole.exe.config" />
  </appSettings>
</configuration>